{
    "build": {
        "@saveall": true
    },
    "format": {
        "jsbeautify": {
            "@formatOnSave": true,
            "@jslinthappy": true,
            "@keeparrayindentation": true
        }
    },
    "general": {
        "@stripws": true
    },
    "instance": {
        "@automaticShutdown": "10080"
    },
    "javascript": {
        "@formatOnSave": true,
        "@use_jsbeautify": true
    },
    "language": {
        "tern_defs": {
            "json()": {
                "browser": {
                    "enabled": true
                },
                "ecma5": {
                    "enabled": true
                },
                "jQuery": {
                    "enabled": true
                }
            }
        }
    },
    "php": {
        "@formatOnSave": true
    },
    "python": {
        "@path": "/usr/local/lib/python3.4/dist-packages:/usr/local/lib/python3.5/dist-packages"
    },
    "run": {
        "configs": {
            "@inited": "true",
            "json()": {}
        }
    },
    "typescript": {
        "@formatOnSave": true
    },
    "welcome": {
        "@first": true
    }
}